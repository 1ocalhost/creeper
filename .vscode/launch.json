{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "App",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/__main__.py",
            "justMyCode": false,
            "env": {
                "PYTHONPYCACHEPREFIX": "${env:TEMP}\\pycache",
                "CREEPER_DBG_MODE": "1",
                "CREEPER_NO_BACKEND": "1",
                "CREEPER_MAIN_PORT": "1081"
            },
            "console": "integratedTerminal"
        },
        {
            "name": "Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "justMyCode": false,
            "console": "integratedTerminal"
        }
    ]
}
