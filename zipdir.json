{
    "output": "out/app.zip",
    "exclude_folder_path": [
        ".vscode",
        "src",
        "scripts"
    ],
    "exclude_file_path": [
        "pack.bat",
        "manifest.json",
        "README.md"
    ],
    "include_file_path": [
        "data/conf/vendor/cacert.pem",
        ["out/src.zip", "src.pyc"],
        ["scripts/install.py", "install.py"]
    ],
    "include_glob": [
        "data/bin/*.exe",
        "data/conf/vendor/rules/*.txt"
    ]
}
